{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/api/oauth/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/api/upload",
      "dest": "/dist/index.js"
    },
    {
      "src": "/api/trpc/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}